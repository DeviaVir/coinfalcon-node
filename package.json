{
  "name": "coinfalcon",
  "description": "Simple API client for the Coinfalcon exchange",
  "homepage": "https://github.com/deviavir/coinfalcon-node",
  "version": "v1.0.5",
  "main": "dist",
  "scripts": {
    "build": "babel src -s -D -d dist",
    "start": "babel-node src/index.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "lint": "eslint src/.",
    "prepublish": "npm run build",
    "pretest": "npm run lint",
    "test": "nyc babel-tape-runner $(find test -name \\*.test.js) | tap-spec"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deviavir/coinfalcon-node.git"
  },
  "author": "Chase Sillevis",
  "license": "MIT",
  "es6": true,
  "engine": ">= 6",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-eslint": "^8.2.1",
    "babel-plugin-istanbul": "^4.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-stage-0": "^6.22.0",
    "babel-tape-runner": "^2.0.1",
    "coveralls": "^3.0.0",
    "eslint": "^4.15.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "nyc": "^11.4.1",
    "sinon": "^5.0.6",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "tape-catch": "^1.0.6",
    "tape-nock": "^1.6.0"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0"
  },
  "keywords": [
    "coinfalcon",
    "coinfalcon.com",
    "exchange",
    "crypto",
    "btc",
    "eth",
    "iota",
    "eur"
  ]
}
